    "compatibility": {
      "typo3-6.2": "1.0.0-1.0.0",
      "typo3-7.6": "1.0.0-2.6.0",
      "typo3-8.7": "2.0.0-3.2.0",
      "typo3-9.5": "3.0.0-4.3.6",
      "typo3-10.4": "4.0.0-x.x.x",
      "typo3-11.5": "5.0.0-x.x.x"
    },
    "typo3/cms": {
      "extension-key": "modules"
    }
  }
