  "keywords": [
    "TYPO3",
    "extension",
    "Backend",
    "Backend modules"
  ],
  "homepage": "https://www.coding.ms",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.4.0 || 8.*",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "codingms/additional-tca": ">=1.5.1",
    "typo3/cms-core": ">=10.4.0,<11.6"
  },
  "authors": [
    {
      "name": "coding.ms GmbH",
      "role": "Developer",
      "homepage": "https://www.coding.ms"
    }
  ],
  "support": {
    "issues": "https://gitlab.com/codingms/typo3-public/modules/-/issues",
